	<Canvas x:Name="BackBorder_Canvas">
		<Rectangle x:Name="BackBorder" Width="715" RadiusX="5" RadiusY="5" Height="24" StrokeThickness="0.5">
			<Rectangle.Stroke>
				<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
					<GradientStop Color="Gray" Offset="0" />
					<GradientStop Color="#00000000" Offset="1" />
				</LinearGradientBrush>
			</Rectangle.Stroke>
			<!--HighlightFill-->
			<Rectangle.Fill>
				<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
					<GradientStop Color="#DDDDDDDC" Offset="0" />
					<GradientStop Color="#00000000" Offset="1" />
				</LinearGradientBrush>
			</Rectangle.Fill>
		</Rectangle>
		<Polygon  x:Name="BackBorder_Polygon" Stroke="Gray" StrokeThickness="0.5" StrokeLineJoin="Round" IsHitTestVisible="False">
			<Polygon.Fill>
				<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
					<GradientStop Color="White" Offset="0" />
					<GradientStop Color="LightSkyBlue" Offset="1" />
				</LinearGradientBrush>
			</Polygon.Fill>
		</Polygon>

	</Canvas>
	<Canvas x:Name="Master_Menu_Border" Canvas.Top="25">
		<Rectangle x:Name="Master_Menu_Rectangle" Width="716" RadiusX="5" RadiusY="5" Height="125" StrokeThickness="3">
